{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/schema.json",
      "destination": "/assets/schema.json"
    },
    {
      "source": "/schema",
      "destination": "/assets/schema.json"
    },
    {
      "source": "/",
      "destination": "/assets/schema.json",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": "Visual Studio Code (desktop)"
        }
      ]
    }
  ]
}
